<div id="dashboard" class="container p-0 mx-auto mt-4">
    <h1 class="py-4">Hello {{userInfo.firstname}} {{userInfo.lastname}}    <button class="buttons btn btn-primary my-2 my-sm-0" type="submit" 
    onclick="window.location='https://bodybywawa.herokuapp.com/logout'">Logout</button></h1>

    <div id="dataDisplay" class="row m-auto p-4 rounded">
        <div class="dashboardData col-sm-12 col-md-4 text-center">
            <h2>Total Calories</h2>
            <div id="totalCalories" class="totalData"></div>
        </div>

        <div class="dashboardData col-sm-12 col-md-4 text-center">
            <h2>Total Weight Moved</h2>
            <div id="totalWeightMoved" class="totalData"></div>
        </div>
        
        <div class="dashboardData col-sm-12 col-md-4 text-center">
            <h2>Total Money Spent</h2>
            <div id="totalMoneySpent" class="totalData"></div>
        </div>
    </div>
</div>